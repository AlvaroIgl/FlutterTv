{
    "editor.codeActionsOnSave": {
        "source.fixAll": "always",
        "source.organizeImports":"always"
    },
    "editor.fontSize":14,
    "command-runner.commands": {
        "pub get": "flutter pub get",
        "build_runner":"dart run build_runner build",
        "build_runner_delete_conflicts":"dart run build_runner build --delete-conflicting-outputs",
        "flutter_gen": "flutter pub run flutter_gen:flutter_gen_command",
    }
}